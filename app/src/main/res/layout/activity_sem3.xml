<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/p"


    tools:context=".Sem3">

    <RelativeLayout
        android:id="@+id/moblie_btn"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="#C2DAFD"
        tools:context=".Sem1">

        <Button
            android:id="@+id/numerical_btn"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="false"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="51dp"
            android:backgroundTint="#AAC9FE"
            android:drawableTop="@drawable/sort_jb9do99fr4fg"
            android:drawableTintMode="screen"
            android:scrollbarAlwaysDrawHorizontalTrack="false"
            android:text="Button" />

        <Button
            android:id="@+id/mobile_btn"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:layout_marginStart="51dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/custom_button"
            android:backgroundTint="#A7C8FD"
            android:drawableTop="@drawable/programming_ra5e25v6zsm6"
            android:text="Button" />

        <Button
            android:id="@+id/d_s_method_btn"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_below="@+id/mobile_btn"
            android:layout_alignParentStart="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="100dp"
            android:background="@color/white"
            android:backgroundTint="#BCCDE5"
            android:drawableTop="@drawable/calculus_105uhwe9xec9"
            android:text="Button" />

        <Button
            android:id="@+id/AI_btn"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_below="@+id/numerical_btn"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="50dp"
            android:background="@color/white"
            android:backgroundTint="#C0D8FC"
            android:drawableBottom="@drawable/infographic_uqcjstntmwzb"
            android:text="Button"
            android:textColorHighlight="#9A2A2A" />

        <Button
            android:id="@+id/button11"
            android:layout_width="wrap_content"
            android:layout_height="107dp"
            android:layout_below="@+id/AI_btn"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="155dp"
            android:layout_marginTop="66dp"
            android:layout_marginEnd="144dp"
            android:backgroundTint="#D3E5FD"
            android:drawableBottom="@drawable/discrete_math_bk3t4y9wa892"
            android:text="Button" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mobile_btn"
            android:layout_alignParentStart="true"
            android:layout_marginStart="58dp"
            android:layout_marginTop="44dp"
            android:text="Mobile Dvelopment"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/numerical_btn"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="41dp"
            android:layout_marginEnd="56dp"
            android:text="Numerical"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="143dp"
            android:layout_height="27dp"
            android:layout_below="@+id/d_s_method_btn"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="46dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="34dp"
            android:layout_marginBottom="203dp"
            android:layout_toStartOf="@+id/textView5"
            android:text="Data Science and methodology"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/AI_btn"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="202dp"
            android:text="Probabilty and Stastics"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="130dp"
            android:layout_marginBottom="83dp"
            android:text="Artificial intelligence"
            android:textStyle="bold" />
    </RelativeLayout>
</RelativeLayout>